{
  "name": "@shop-react-azure-terraform/client",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "deploy": "npm run build && az storage blob upload-batch -s './dist' -d '$web' --account-name 'artemganevstorageacc0' --overwrite",
    "lint": "eslint 'src/**/*.{tsx,ts}' --quiet --max-warnings 1",
    "lint:fix": "eslint 'src/**/*.{tsx,ts}' --fix ",
    "lint:warn": "eslint 'src/**/*.{tsx,ts}'",
    "preview": "npm run build && vite preview",
    "start": "vite",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@mui/icons-material": "5.15.14",
    "@mui/material": "5.15.14",
    "@mui/styles": "5.15.14",
    "@types/yup": "^0.29.14",
    "axios": "1.6.8",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.1",
    "react-router-dom": "^6.3.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.2",
    "@testing-library/user-event": "14.5.2",
    "@types/react": "18.2.73",
    "@types/react-dom": "18.2.23",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/ui": "1.4.0",
    "c8": "9.1.0",
    "jsdom": "24.0.0",
    "msw": "2.2.13",
    "vite": "5.2.7",
    "vitest": "1.4.0"
  },
  "overrides": {
    "react": "$react"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
