{
  "name": "@shop-react-azure-terraform/client",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc && vite build",
    "deploy": "npm run build && az storage blob upload-batch -s './dist' -d '$web' --account-name 'learnazurestorageaccount00' --overwrite",
    "lint": "eslint 'src/**/*.{tsx,ts}' --quiet --max-warnings 1",
    "lint:fix": "eslint 'src/**/*.{tsx,ts}' --fix ",
    "lint:warn": "eslint 'src/**/*.{tsx,ts}'",
    "preview": "npm run build && vite preview",
    "start": "vite",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@mui/icons-material": "5.15.14",
    "@mui/material": "5.15.14",
    "@mui/styles": "5.15.14",
    "@types/yup": "^0.29.14",
    "axios": "1.6.8",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.1",
    "react-router-dom": "^6.3.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitest/ui": "^0.18.0",
    "c8": "^7.11.3",
    "jsdom": "^20.0.0",
    "msw": "^0.43.1",
    "vite": "^2.9.13",
    "vitest": "^0.17.1"
  },
  "overrides": {
    "react": "$react"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
